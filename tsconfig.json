{
	"extends": "./.svelte-kit/tsconfig.json",
	"include": [
		"./src/**/*",
		"./src/**/*.svelte",
		"./unocss.config.ts"
	],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"strictPropertyInitialization": false,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "node",
		"module": "esnext",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "esnext"
	}
}
